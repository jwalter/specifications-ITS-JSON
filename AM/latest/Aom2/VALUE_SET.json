{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://www.openEHR.org/releases/ITS-JSON/Release-1.0.0/AM/latest/Aom2/VALUE_SET.json",
    "title": "VALUE_SET",
    "description": "Representation of a flat value set within the archetype terminology.",
    "$comments": "Defined in Aom2 IM, inherits from TERMINOLOGY_RELATION",
    "type": "object",
    "properties": {
        "id": {
            "description": "Code of source term of this relation.",
            "$ref": "https://www.openEHR.org/releases/ITS-JSON/Release-1.0.0/BASE/latest/Foundation_types/all.json#/definitions/String"
        },
        "members": {
            "description": "List of target terms in this relation.",
            "type": "array",
            "items": {
                "$ref": "https://www.openEHR.org/releases/ITS-JSON/Release-1.0.0/BASE/latest/Foundation_types/all.json#/definitions/String"
            },
            "minItems": 1
        }
    },
    "required": [
        "id",
        "members"
    ],
    "$inherits": [
        "all.json#/definitions/TERMINOLOGY_RELATION"
    ]
}