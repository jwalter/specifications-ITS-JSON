{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://www.openEHR.org/releases/ITS-JSON/Release-1.0.0/components/PROC/latest/Materialised/M_CALLBACK.json",
    "title": "M_CALLBACK",
    "description": "Class representing the runtime tracked form of a callback for currently blocked tasks.",
    "$comments": "Defined in Materialised IM",
    "type": "object",
    "properties": {
        "task_item": {
            "description": "Reference to the Task Item blocked waiting on this callback.",
            "$ref": "all.json#/definitions/M_TASK"
        },
        "dispatch_info": {
            "description": "Dispatch object defining actions on receipt of callback.",
            "$ref": "https://www.openEHR.org/releases/ITS-JSON/Release-1.0.0/components/PROC/latest/Definition/all.json#/definitions/DISPATCHABLE_ACTION"
        },
        "other_details": {
            "$ref": "https://www.openEHR.org/releases/ITS-JSON/Release-1.0.0/components/BASE/latest/Foundation_types/all.json#/definitions/Hash_of_String_String"
        }
    },
    "required": [
        "task_item",
        "dispatch_info",
        "other_details"
    ]
}