{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://www.openEHR.org/releases/ITS-JSON/components/RM/latest/Ehr_extract/X_CONTRIBUTION.json",
    "title": "X_CONTRIBUTION",
    "description": "Serialised form of Contribution for an Extract.",
    "$comments": "Defined in Ehr_extract IM",
    "type": "object",
    "properties": {
        "uid": {
            "description": "Uid of Contribution in source system.",
            "$ref": "https://www.openEHR.org/releases/ITS-JSON/components/BASE/latest/Base_types/all.json#/definitions/HIER_OBJECT_ID"
        },
        "audit": {
            "description": "Audit of Contribution in source system.",
            "$ref": "https://www.openEHR.org/releases/ITS-JSON/components/RM/latest/Common/all.json#/definitions/AUDIT_DETAILS"
        },
        "versions": {
            "description": "Serialised Versions from Contribution in source system.",
            "type": "array",
            "items": {
                "$ref": "https://www.openEHR.org/releases/ITS-JSON/components/RM/latest/Common/all.json#/definitions/VERSION"
            }
        }
    },
    "required": [
        "uid",
        "audit"
    ]
}