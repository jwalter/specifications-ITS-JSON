{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://www.openEHR.org/releases/ITS-JSON/Release-1.0.0/BASE/latest/Foundation_types/Container.json",
    "title": "Container",
    "description": "Abstract ancestor of container types whose items are addressable in some way.",
    "$comments": "Defined in Foundation_types IM, abstract",
    "type": "array",
    "properties": {
        "_type": {
            "enum": {
                "0": "Hash",
                "1": "List",
                "2": "Set",
                "3": "Array",
                "4": "Hash<String, Hash<String, Hash<String, String>>>",
                "5": "Hash<String, Hash<String, String>>",
                "6": "Hash<String, String>",
                "7": "Hash<String, List<String>>",
                "8": "Hash<String, TRANSLATION_DETAILS>",
                "9": "Hash<String, RESOURCE_DESCRIPTION_ITEM>",
                "10": "Hash<String, ARCHETYPE_TERMINOLOGY>",
                "11": "Hash<String, VALUE_SET>",
                "12": "Hash<String, Hash<String, ARCHETYPE_TERM>>",
                "13": "Hash<String, ARCHETYPE_TERM>",
                "14": "Hash<String, Uri>",
                "15": "Hash <String, Hash<String, Uri>>",
                "16": "Hash<String, AOM_PROPERTY_MAPPING>",
                "17": "Hash<String, AOM_TYPE_MAPPING>",
                "19": "List<String>",
                "20": "Set<String>"
            }
        }
    },
    "required": [
        "_type"
    ],
    "$abstract": true,
    "$children": [
        "all.json#/definitions/Hash",
        "all.json#/definitions/List",
        "all.json#/definitions/Set",
        "all.json#/definitions/Array"
    ],
    "$functions": {
        "has": {
            "description": "Test for membership of a value.",
            "parameters": {
                "v": {
                    "$ref": "all.json#/definitions/T"
                }
            },
            "required_parameters": [
                "v"
            ],
            "return": {
                "$ref": "all.json#/definitions/Boolean"
            }
        },
        "count": {
            "description": "Number of items in container.",
            "return": {
                "$ref": "all.json#/definitions/Integer"
            }
        },
        "is_empty": {
            "description": "True if container is empty.",
            "return": {
                "$ref": "all.json#/definitions/Boolean"
            }
        }
    }
}